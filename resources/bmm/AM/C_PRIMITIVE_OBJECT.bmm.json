{
    "name": "C_PRIMITIVE_OBJECT",
    "documentation": "Constraint on a primitive type.",
    "ancestors": [
        "C_DEFINED_OBJECT"
    ],
    "properties": {
        "item": {
            "_type": "P_BMM_SINGLE_PROPERTY",
            "name": "item",
